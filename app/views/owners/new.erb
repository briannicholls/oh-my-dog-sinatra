<h1>Create Owner</h1>

<form action="/owners" method="post">
<table>
  <tr>
    <td><label for="owner[first_name]">First Name:</label></td>
    <td><input type="text" name="owner[first_name]"></td>
  </tr>

  <tr>
    <td><label for="owner[last_name]">Last Name:</label></td>
    <td><input type="text" name="owner[last_name]"></td>
  </tr>

  <tr>
    <td><label for="owner[phone]">Phone #:</label></td>
    <td><input type="tel" name="owner[phone]"></td>
  </tr>

  <tr>
    <td><label for="owner[phone2]">Phone 2 #:</label></td>
    <td><input type="tel" name="owner[phone2]"></td>
  </tr>

  <tr>
    <td><label for="owner[address]">Address:</label></td>
    <td><input type="text" name="owner[address]"></td>
  </tr>

  <tr>
    <td><label for="owner[apartment_number]">Apartment #:</label></td>
    <td><input type="text" name="owner[apartment_number]"></td>
  </tr>

  <tr>
    <td><label for="owner[zip_code]">Zip Code:</label></td>
    <td><input type="text" name="owner[zip_code]"></td>
  </tr>

  <tr>
    <td><label for="owner[instructions]">Entry Instructions:</label></td>
    <td><input type="text" name="owner[instructions]"></td>
  </tr>

  <tr>
    <td><label for="owner[door_code]">Door Code:</label></td>
    <td><input type="text" name="owner[door_code]"></td>
  </tr>

  <tr>
    <td><label for="owner[lockbox_code]">Lockbox Code:</label></td>
    <td><input type="text" name="owner[lockbox_code]"></td>
  </tr>

  <tr>
    <td></td>
    <td><input type="submit"></td>
  </tr>
</table>
</form>
