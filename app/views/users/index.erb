<table class="w3-table w3-bordered">
  <tr>
    <th colspan="3">
      <a class="w3-btn w3-green w3-bar" href="/users/new">Create New Walk</a>
    </th>
  </tr>
  <tr>
    <th><h1>All Users</h1></th>
  </tr>
  <% User.all.each do |user| %>
  <tr>
    <td>
      <a href="/users/<%= user.id %>"><%= "#{user.first_name} #{user.last_name}" %></a>
    </td>
  </tr>
  <% end %>
</table>
